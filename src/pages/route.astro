---
import PublicLayout from '@/layouts/PublicLayout.astro';
import CalculateRouteSection from '@/route/sections/CalculateRouteSection.astro';
const robots = ["index", "follow"];
const title = "PECO | Calcular Ruta";
const description = "Este espacio está reservado para la funcionalidad de calcular rutas de PECO, una plataforma que conecta a los consumidores con productos del mar frescos y de calidad.";
const keywords = ["PECO", "calcular ruta", "productos del mar", "cadena de frío", "transparencia en el origen"];
const url = "https://peco-aptrv/route.vercel.app";
const imageOG = "/card-twt.webp";
const imagetwt = "/card-twt.webp";
---

<PublicLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
    <CalculateRouteSection />
</PublicLayout>
